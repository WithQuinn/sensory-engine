{
  "name": "sensory-engine",
  "version": "0.1.0",
  "description": "Emotional synthesis from travel moments via photos, audio, and venue data",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest run --exclude 'tests/e2e/**' --exclude 'tests/contract/**'",
    "test:unit:watch": "vitest --exclude 'tests/e2e/**' --exclude 'tests/contract/**'",
    "test:integration": "vitest run tests/integration/**",
    "test:e2e": "playwright test",
    "test": "npm run test:unit && npm run test:integration",
    "benchmark": "npx tsx benchmarks/establish-baselines.ts",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.28.1",
    "@hookform/resolvers": "^3.3.4",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
