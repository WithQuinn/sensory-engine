{
  "name": "sensory-engine",
  "version": "0.1.0",
  "description": "Emotional synthesis from travel moments via photos, audio, and venue data",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest run tests/unit tests/integration",
    "test:unit:watch": "vitest tests/unit tests/integration",
    "test:integration": "vitest run tests/integration/**",
    "test:e2e": "playwright test",
    "test": "npm run test:unit && npm run test:integration",
    "benchmark": "npx tsx benchmarks/establish-baselines.ts",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^1.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
